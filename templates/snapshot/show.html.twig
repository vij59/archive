{% extends 'base.html.twig' %}

{% block title %}Snapshot{% endblock %}

{% block body %}
    <h1>Snapshot</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ snapshot.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ snapshot.name }}</td>
            </tr>
            <tr>
                <th>Siren</th>
                <td>{{ snapshot.siren }}</td>
            </tr>
            <tr>
                <th>ImmatriculationCity</th>
                <td>{{ snapshot.immatriculationCity }}</td>
            </tr>
            <tr>
                <th>ImmatriculationDate</th>
                <td>{{ snapshot.immatriculationDate ? snapshot.immatriculationDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Capital</th>
                <td>{{ snapshot.capital }}</td>
            </tr>
            <tr>
                <th>LegalForm</th>
                <td>{{ snapshot.legalForm }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('snapshot_index') }}">back to list</a>

    {{ include('snapshot/_delete_form.html.twig') }}
{% endblock %}
